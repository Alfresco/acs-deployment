								"type": "text/javascript",
								"exec": [
									"var protocol = pm.environment.get(\"protocol\");",
									"var path = pm.environment.get(\"path\");",
									"var alfport = pm.environment.get(\"alfport\");",
									"var shareport = pm.environment.get(\"shareport\");",
									"var solrport = pm.environment.get(\"solrport\");",
									"",
									"if( !protocol) {",
									"    protocol = \"http\";",
									"}",
									"if( !path) {",
									"    path = \"localhost\";",
									"}",
									"if( !alfport) {",
									"    alfport = \"8082\";",
									"}",
									"if( !shareport) {",
									"    shareport = \"8080\";",
									"}",
									"if( !solrport) {",
									"    solrport = \"8083\";",
									"}",
									"",
									"pm.environment.set(\"protocol\", protocol);",
									"pm.environment.set(\"path\", path);",
									"pm.environment.set(\"alfport\", alfport);",
									"pm.environment.set(\"shareport\", shareport);",
									"pm.environment.set(\"solrport\", solrport);"
								]
